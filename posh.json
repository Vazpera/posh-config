{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#007ACC",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
            "type": "status",
            "style": "diamond",
            "foreground": "#211A1E",
            "background": "#5BC0EB",
            "background_templates": [
              "{{ if .Error }}#D26D9E{{ end }}"
            ],
            "trailing_diamond": "ÓÇ∞",
            "template": "<#211A1E>ÓÇ∞</> {{ if .Error }}√ó{{ else }}‚úì{{ end }}",
            "properties": {
              "always_enabled": true
            }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#211A1E",
          "background": "#c3423f",
          
          "template": " {{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }} "
        },
        {
          "properties": {
            "style": "folder"
          },
          "foreground": "#211A1E",
          "powerline_symbol": "\ue0b0",
          "background": "#E09546",
          "template":"{{ if .Segments.Contains \"Project\" }}{{ else }} {{ .Path }} {{ end }}",
          "type": "path",
          "style": "accordion"
        },
        {
          "type": "project",
          "style": "accordion",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#211A1E",
          "background": "#FDE74C",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}v{{.Version}} {{ if .Name }}{{ .Name }}{{ end }}{{ end }}{{ end }} "
        },
        {
          "type": "git",
          "style": "accordion",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#211A1E",
          "background": "#9BC53D",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        }
      ]
    }
  ],
  "version": 3
}
