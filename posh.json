{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#007ACC",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
            "type": "status",
            "style": "diamond",
            "background": "#211A1E00",
            "foreground": "#5BC0EB",
            "foreground_templates": [
              "{{ if .Error }}#D26D9E{{ end }}"
            ],
            "trailing_diamond": "",
            "template": "<#211A1E00>\ue0b1</> {{ if .Error }}{{ else }}{{ end }}",
            "properties": {
              "always_enabled": true
            }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#c3423f",
          "trailing_diamond": "",
          "template": "{{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }}"
        },
        {
          "type": "text",
          "foreground": "#c3423f",
          "template": " \ue0b1"
        },
        {
          "properties": {
            "style": "folder"
          },
          "powerline_symbol": "",
          "foreground": "#E09546",
          "template":"{{ if or (.Segments.Contains \"Project\") (.Segments.Contains \"Git\") }}{{ else }} {{ .Path }} {{ end }}<#e09546>\ue0b1</>",
          "type": "path",
          "style": "accordion"
        },
        {
          "type": "project",
          "style": "accordion",
          "background": "#211A1E00",
          "foreground": "#FDE74C",
          "powerline_symbol": "",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}v{{.Version}} {{ if .Name }}{{ .Name }}{{ end }}{{ end }}{{ end }} "
        },
        {
          "type": "text",
          "foreground": "#fde74c",
          "template": "\ue0b1"
        },
        {
          "type": "git",
          "style": "accordion",
          "powerline_symbol": "",
          "background": "#211A1E00",
          "foreground": "#9BC53D",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#675BEB{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#E09546{{ end }}",
            "{{ if gt .Ahead 0 }}#9952B7{{ end }}",
            "{{ if gt .Behind 0 }}#9952B7{{ end }}"
          ],
          "template": " {{ .RepoName }} {{ .HEAD }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }} \ue0b1",

          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        },
        {
          "type": "text",
          "foreground": "#9BC53D",
          "template": "{{ if .Segments.Contains \"Git\" }}{{ else }}\ue0b1{{ end }}"

        
        }
      ]
    }
  ],
  "version": 3
}
